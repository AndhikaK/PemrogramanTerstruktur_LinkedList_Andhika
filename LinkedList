#include <iostream>
#include <cstdlib>
#include <conio.h>
using namespace std;

struct TNode{
    int data;
    TNode *next;
};

class Node{
	public:
    	int data;
		Node* next;
		Node* posisi;
};

TNode *head, *tail;

void headtail(){
    head=NULL;
    tail=NULL;
}

int isEmpty(){
	if(tail==NULL) 
		return 1;
	else 
		return 0;
}

void insertBelakang(int databaru){
	TNode *baru;
	baru=new TNode;
	baru->data=databaru;
	baru->next=NULL;
	if(isEmpty()==1){
		head=baru;
		tail=baru;
		tail->next=NULL;
	} else {
		tail->next = baru;
	    tail=baru;
	}
	cout<<"Data masuk"<<endl;
}
