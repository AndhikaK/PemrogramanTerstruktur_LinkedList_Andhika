#include <iostream>
#include <cstdlib>
#include <conio.h>
using namespace std;

struct TNode{
    int data;
    TNode *next;
};

class Node{
	public:
    	int data;
		Node* next;
		Node* posisi;
};

TNode *head, *tail;

void headtail(){
    head=NULL;
    tail=NULL;
}

int isEmpty(){
	if(tail==NULL) 
		return 1;
	else 
		return 0;
}
